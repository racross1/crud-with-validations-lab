<h1>Songs</h1>
<table>
    <tr>
        <th scope="col">Title</th>
        <th scope="col"> Release Year</th>
        <th scope="col"> Released? </th>
        <th scope="col"> Genre</th>
        <th scope="col"> Artist</th>
    </tr>

<% @songs.each do |song|%>
    <tr> 
    <td> <%= song.title %> </td>
    <td> <%= song.release_year %></td>
    <td> <%= song.released %></td>
    <td> <%= song.genre %></td>
    <td> <%= song.artist_name %></td>
    </tr>
<% end %>
</table>



